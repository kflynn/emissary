enableLegacyVersions: false     # Set to true to include at least v2 versions and the
                                # conversion webhook.
enableV1: false                 # Set to true with enableLegacyVersions true to also
                                # include v1 versions. No effect without
                                # enableLegacyVersions.

emissary:
  namespace: emissary

apiext:
  # apiext docker repo
  repository: @imageRepo@
  # apiext docker tag
  tag: @version@
  pullPolicy: IfNotPresent
